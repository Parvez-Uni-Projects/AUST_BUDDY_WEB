


@{
    ViewBag.Title = "Course";
}

<h1 class="display-5 fw-bold">Software Devlopment</h1>

@model List<AUST_BUDDY_WEB.Models.YouTubeVideo>


@if (Model.Count > 0)
{
    <section style="margin:5%">
        @foreach (var video in Model)
        {
        
            <div class="col-md-12">
                <h2>@video.Title</h2>
                <div class="video-container">
                    <!-- Assuming you have a property for the video URL in the YouTubeVideo model -->
                    <iframe width="560" height="315" src="https://www.youtube.com/embed/@video.VideoId" frameborder="0" allowfullscreen></iframe>
                </div>
            </div>
        }
     </section>
}
else
{
    <p>No videos found.</p>
}
